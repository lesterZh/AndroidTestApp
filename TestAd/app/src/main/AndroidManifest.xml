<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zhhtao.testad">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.zhhtao.activity.IndicatorTestActivity"
            android:theme="@style/Theme.AppCompat" />
        <activity android:name="com.zhhtao.activity.AnimatorTestActivity" />
        <activity android:name="com.zhhtao.activity.IndicatorAdvanceActivity" />
        <activity android:name="com.zhhtao.activity.ScrollTestActivity" />
        <activity android:name="com.zhhtao.activity.DrawTestActivity" />
        <activity android:name="com.zhhtao.activity.CalendarTestActivity" />
        <activity android:name="com.zhhtao.activity.RetrofitTestActivity" />
        <activity android:name="com.zhhtao.activity.SwipeRefreshLayoutTestActivity"></activity>
    </application>

</manifest>
